{
  "name": "rsk-explorer-api",
  "version": "1.2.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npx nodemon src/api | ./node_modules/.bin/bunyan -o short",
    "blocks:balances": "npx babel-node src/services/blocks/blocksBalances.js | ./node_modules/.bin/bunyan -o short",
    "blocks:checker": "npx babel-node src/services/blocks/blocksChecker.js | ./node_modules/.bin/bunyan -o short",
    "blocks:listener": "npx babel-node src/services/blocks/blocksListener.js | ./node_modules/.bin/bunyan -o short",
    "blocks:requester": "npx babel-node src/services/blocks/blocksRequester.js | ./node_modules/.bin/bunyan -o short",
    "blocks:router": "npx babel-node src/services/blocks/blocksRouter.js | ./node_modules/.bin/bunyan -o short",
    "blocks:stats": "npx babel-node src/services/blocks/blocksStats.js | ./node_modules/.bin/bunyan -o short",
    "blocks:status": "npx babel-node src/services/blocks/blocksStatus.js | ./node_modules/.bin/bunyan -o short",
    "blocks": "npm run blocks:balances & npm run blocks:checker & npm run blocks:listener & npm run blocks:requester & npm run blocks:router & npm run blocks:stats & npm run blocks:status",
    "build": "npm run lint && npm run clean && npm run api-docs && ./node_modules/.bin/babel src -d dist --copy-files",
    "clean": "rm -rf dist && rm -f .node-xmlhttprequest-sync*",
    "md": "mkdir dist",
    "status": "npx nodemon src/tools/statusClient.js",
    "abi": "npx nodemon src/lib/ContractParser/compileJsonAbis.js",
    "lint": "npx eslint src/**/* --quiet",
    "test": "npx mocha",
    "api-docs": "npx babel-node src/api/makeOpenApiSpec.js > public/swagger.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rsksmart/rsk-explorer-api.git"
  },
  "keywords": [
    "rsk",
    "explorer"
  ],
  "author": "emi",
  "license": "MIT",
  "homepage": "https://github.com/rsksmart/rsk-explorer-api",
  "dependencies": {
    "@rsksmart/nod3": "^0.4.1",
    "@rsksmart/rsk-contract-parser": "git+https://git@github.com/rsksmart/rsk-contract-parser.git",
    "@rsksmart/rsk-js-cli": "^1.0.0",
    "@rsksmart/rsk-openapi-ui": "git+https://git@github.com/rsksmart/rsk-openapi-ui.git",
    "@rsksmart/rsk-utils": "^1.1.0",
    "bignumber.js": "^7.2.1",
    "body-parser": "^1.19.0",
    "bunyan": "^2.0.4",
    "express": "^4.17.1",
    "google-protobuf": "^3.13.0",
    "grpc": "^1.24.10",
    "mongodb": "^3.6.1",
    "rlp": "^2.2.5",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@babel/register": "^7.13.16",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "grpc-tools": "^1.9.1",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "swagger-jsdoc": "^3.2.9",
    "swagger-markdown": "^1.4.2"
  }
}
